<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clt.wechatordering.mapper.OrderDetailMapper">

    <select id="findAllOrder" resultType="com.clt.wechatordering.pojo.OrderDetail">
        select * from order_detail
    </select>

    <select id="findOrderById" resultType="com.clt.wechatordering.pojo.OrderDetail">
        select * from order_detail where detail_id = #{id};
    </select>
</mapper>
