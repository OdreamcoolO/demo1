<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clt.wechatordering.mapper.ProductCategoryMapper">

    <select id="findById" resultType="com.clt.wechatordering.pojo.ProductCategory">
        select * from product_category where category_id = #{id}
    </select>

    <select id="findAll" resultType="com.clt.wechatordering.pojo.ProductCategory">
        select * from product_category
    </select>

</mapper>
